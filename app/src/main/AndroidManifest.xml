<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="miranda.william.qrcodegenerator">

    <application
        android:name="miranda.william.qrcodegenerator.SignatureApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Signature">

        <activity
            android:name="miranda.william.qrcodegenerator.ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Signature.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>